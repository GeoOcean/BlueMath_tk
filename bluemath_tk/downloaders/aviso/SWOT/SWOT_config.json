      "level": "L3",
      "variant": "Expert",
      "ftp_base_path": "/swot_products/l3_karin_nadir/l3_lr_ssh/v2_0_1/Expert/",
      "ftp_server": "ftp-access.aviso.altimetry.fr",
      "types": ["oceanography", "altimetry"],
      "mandatory_fields": ["variables", "lon_range", "lat_range"],
      "optional_fields": ["force", "dry_run"],
      "template": {
        "variables": ["ssha_filtered", "time"],
        "lon_range": [-15.0, 40.0],
        "lat_range": [25.0, 50.0]
      }
    }
  },
