      "dataset": "swot-l3-expert"
    },
    "time": {
      "nc_name": "time",
      "long_name": "Time",
      "description": "Time coordinate",
      "units": "seconds since 2000-01-01 00:00:00",
      "type": "coordinate",
      "dataset": "swot-l3-expert"
    },
    "longitude": {
      "nc_name": "longitude",
      "long_name": "Longitude",
      "description": "Longitude coordinate",
      "units": "degrees_east",
      "type": "coordinate",
      "dataset": "swot-l3-expert"
    },
    "latitude": {
      "nc_name": "latitude",
      "long_name": "Latitude",
      "description": "Latitude coordinate",
      "units": "degrees_north",
      "type": "coordinate",
      "dataset": "swot-l3-expert"
    },
    "ssha": {
      "nc_name": "ssha",
      "long_name": "Sea Surface Height Anomaly",
